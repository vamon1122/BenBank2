<UserControl x:Class="BenBank2.Controls.UserControl_FinancialEntity"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:BenBank2.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="200" d:DesignWidth="250" BorderThickness="5" BorderBrush="Firebrick" Margin="0,0,0,0" Width="250">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition></ColumnDefinition>
            <ColumnDefinition Width="2*"></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <TextBlock Grid.Column="1" Name="FinancialEntity_Name" FontSize="25" TextAlignment="Center"></TextBlock>
        <TextBlock Grid.Column="1" Grid.Row="1" Name="FinancialEntity_Balance" FontSize="25" TextAlignment="Center"></TextBlock>
    </Grid>
</UserControl>
